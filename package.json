{
  "name": "koala-t",
  "version": "0.1.0",

  "author": "Josh Perez <josh@goatslacker.com>",
  "description": "Koala-t checks for code quality",

  "bin": {
    "koala-t": "./bin/koala-t"
  },

  "scripts": {
    "pretest": "rm -f test/*-test.js",
    "test": "./bin/koala-t",
    "prepublish": "docco koala.js lib/*.js lib/utils/*.js"
  },

  "main": "./lib/koala.js",

  "repository": {
    "type": "git",
    "url": "https://github.com/goatslacker/koala-t"
  },

  "keywords": ["code quality", "lint", "test"],

  "license": "MIT",

  "dependencies": {
    "coveraje": "latest",
    "typedjs": "latest",
    "dune": "latest",
    "minimatch": "latest",
    "coffeelint": "latest",
    "commander": "latest"
  },

  "devDependencies": {
    "coffee-script": "latest",
    "docco": "latest",
    "vows": "latest"
  }
}
